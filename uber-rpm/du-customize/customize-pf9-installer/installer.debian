PKG_MANAGER="dpkg"
QUERY_PKGS="${PKG_MANAGER} -l"
INSTALL="${PKG_MANAGER} --install"
EXT="deb"

function distro_install()
{
    echo "Installing Debian packages"
    apt-get install -y python-setuptools
    if [[ $? != "0" ]]; then
        echo "python-setuptools installation failed"
        return 1
    fi
}
